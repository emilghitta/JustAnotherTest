<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="NameFilterTests" verbose="1" >
    <listeners>
        <listener class-name="core.Listeners"/>
    </listeners>
    <test name="NameFilterTest" >
        <parameter name="browser" value="Edge"/>
        <parameter name="pageURL" value="https://rahulshettyacademy.com/seleniumPractise/#/offers"/>
        <classes>
            <class name="GeneralSeleniumFunctionalityTests.NameFilter">
                <methods>
                    <include name="nameFilterTest"/>
                </methods>
            </class>
        </classes>
    </test>


</suite>